<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE module PUBLIC '-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN'
 'http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd'>

<module rename-to='sandbox'>
  <inherits name='com.ventana.gwt.mobilebrowser.MobileBrowser' />

	<source path='sandbox' />

	<entry-point class='com.ventana.gwt.mobilebrowser.sandbox.SwipeRecognizerExample' />

	<add-linker name="xsiframe"/>

	<set-property name='user.agent' value='safari' />
  <set-configuration-property name='CssResource.style' value='pretty' />
</module>