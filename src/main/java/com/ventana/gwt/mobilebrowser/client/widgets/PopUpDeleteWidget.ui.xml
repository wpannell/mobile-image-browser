<!DOCTYPE ui:UiBinder SYSTEM 'http://dl.google.com/gwt/DTD/xhtml.ent'>
<ui:UiBinder
  xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:v='urn:import:com.ventana.gwt.mobilebrowser.client.widgets'
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
	>
	 <b:Modal ui:field='modal' styleName='modal hide fade'
     backdrop='STATIC' keyboard='true' animation='false' closeVisible='false' >
       <b:Heading ui:field='question' size="2"></b:Heading>
    <b:ModalFooter>
      <b:Button ui:field='okButton' type='PRIMARY'>OK</b:Button>
      <b:Button ui:field='cancelButton' type='DANGER'>CANCEL</b:Button>
    </b:ModalFooter>
  </b:Modal>
</ui:UiBinder>
